{
  "comments": {
    "lineComment": "REM"
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\""
    }
  ],
  "surroundingPairs": [
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(::\\s*|REM\\s+)#region",
      "end": "^\\s*(::\\s*|REM\\s+)#endregion"
    }
  }
}