{
  "indentationRules": {
    "decreaseIndentPattern": "(\\\\end{)|(\\\\begin{document})",
    "increaseIndentPattern": "(\\\\begin{(?!(document|macrocode)))|(\\\\documentclass)"
  },
  "folding": {
    "markers": {
      "start": "^\\s*%\\s*#?region\\b.*",
      "end": "^\\s*%\\s*#?endregion\\b.*"
    }
  },
  "comments": {
    "lineComment": "^^A"
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "`",
      "close": "'"
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ],
    [
      "$",
      "$"
    ],
    [
      "'",
      "'"
    ],
    [
      "`",
      "'"
    ]
  ]
}